@model IEnumerable<WBlog.Models.Etiket>

<div class="sidebox widget">
    <h3 class="widget-title section-title">Etiketler</h3>
    <ul class="tag-list">
        @foreach (WBlog.Models.Etiket etiket in Model)
        {
            <li><a href="/Etiket/Index/@etiket.id">@etiket.adi (@etiket.Makale.Count)</a></li>
        }
        
        
    </ul>
    <!-- /.tag-list -->

</div>
